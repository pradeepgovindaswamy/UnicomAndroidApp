            app:backgroundTint="@android:color/background_dark"
            app:srcCompat="@drawable/enew" />

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/j"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
